<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Description>Kledex Memory Cache Provider</Description>
    <PackageTags>$(PackageTags), caching, memory</PackageTags>
    <PackageLicenseExpression>$(PackageLicense)</PackageLicenseExpression>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Caching.Abstractions" Version="3.1.5" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Kledex\Kledex.csproj" />
  </ItemGroup>

</Project>
